{
    vsp += grav;
}
//Horizontal Collision
if (place_meeting(x+hsp,y,par_tilesets))
{
    while(!place_meeting(x+(hsp),y,par_tilesets))
    {
        x += sign(hsp)
    }
    hsp = 0;
    
   // dir *= -1;
}
